{
  "name": "immigration-app-backend",
  "version": "1.0.0",
  "description": "Backend for immigration assistance application",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --watch dist/index.js",
    "dev": "nodemon --watch src -e ts,json --exec 'ts-node' src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "node --import tsx --test tests/**/*.test.ts",
    "test:unit": "node --import tsx --test tests/unit/**/*.test.ts",
    "test:integration": "node --import tsx --test tests/integration/**/*.test.ts",
    "test:e2e": "node --import tsx --test tests/e2e/**/*.test.ts",
    "test:watch": "node --import tsx --test --watch tests/**/*.test.ts",
    "test:coverage": "node --import tsx --test --coverage tests/**/*.test.ts"
  },
  "engines": {
    "node": ">=22.11.0",
    "npm": ">=10.9.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cookie": "^0.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.17.9",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3"
  }
}
